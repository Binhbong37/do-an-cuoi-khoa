<div class="Page navigation example" style="align-items: center">
    <ul class="pagination">
        <% if(currentPage !== 1 && previousPage !== 1) { %>
        <li class="page-item">
            <a href="/admin/user/?page=1" class="page-link">1</a>
        </li>
        <% } %> <% if(hashPreviousPage) { %>
        <li class="page-item">
            <a class="page-link" href="/admin/user/?page=<%= previousPage %>"
                ><%= previousPage %></a
            >
        </li>
        <% } %>
        <li class="page-item">
            <a
                class="page-link"
                href="/admin/user/?page=<%= currentPage %>"
                class="btn btn-link"
                ><%= currentPage %></a
            >
        </li>
        <% if(hashNextPage) { %>
        <li class="page-item">
            <a class="page-link" href="/admin/user/?page=<%= nextPage %>"
                ><%= nextPage %></a
            >
        </li>
        <% } %> <% if(lastPage !== currentPage && nextPage !== lastPage) { %>
        <li class="page-item">
            <a class="page-link" href="/admin/user/?page=<%= lastPage %>"
                ><%= lastPage %></a
            >
        </li>
        <% } %>
    </ul>
</div>
